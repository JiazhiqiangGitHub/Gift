<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/ll_pop_two"
    android:background="#FFFFFF">
    <FrameLayout
        android:layout_marginTop="10dp"
        android:background="#ffffffff"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView

            android:id="@+id/pop_two_back"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:padding="10dp"
            android:src="@mipmap/back"/>
        <TextView

            android:layout_gravity="center"
            android:text="选择身份"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <ImageView
            android:id="@+id/pop_two_close"
            android:layout_marginLeft="150dp"
            android:src="@mipmap/backblack"
            android:layout_marginRight="10dp"
            android:layout_marginTop="5dp"
            android:layout_width="20dp"
            android:layout_height="20dp"/>

    </FrameLayout>
    <FrameLayout
        android:background="#ffffffff"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
        <TextView
            android:id="@+id/tv_one"
            android:layout_gravity="center_vertical"
            android:padding="10dp"
            android:text="初中生"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <ImageButton
            android:id="@+id/one"
            android:padding="10dp"
            android:layout_gravity="center_vertical|right"
            android:background="#ffffff"
            android:src="@drawable/unselect_btn"
            android:layout_width="40dp"
            android:scaleType="fitXY"
            android:layout_height="40dp"/>

    </FrameLayout>
    <FrameLayout

        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
        <TextView
            android:id="@+id/tv_two"
            android:layout_gravity="center_vertical"
            android:padding="10dp"
            android:text="高中生"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <ImageButton
            android:id="@+id/two"
            android:padding="10dp"
            android:layout_gravity="center_vertical|right"
            android:background="#ffffff"
            android:src="@drawable/unselect_btn"
            android:layout_width="40dp"
            android:scaleType="fitXY"
            android:layout_height="40dp"/>

    </FrameLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
        <TextView
            android:id="@+id/tv_three"
            android:layout_gravity="center_vertical"
            android:padding="10dp"
            android:text="大学生"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <ImageButton
            android:id="@+id/three"
            android:padding="10dp"
            android:layout_gravity="center_vertical|right"
            android:background="#ffffff"
            android:src="@drawable/unselect_btn"
            android:layout_width="40dp"
            android:scaleType="fitXY"
            android:layout_height="40dp"/>

    </FrameLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
        <TextView
            android:id="@+id/tv_four"
            android:layout_gravity="center_vertical"
            android:padding="10dp"
            android:text="职场新人"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <ImageButton
            android:id="@+id/four"
            android:padding="10dp"
            android:layout_gravity="center_vertical|right"
            android:background="#ffffff"
            android:src="@drawable/unselect_btn"
            android:layout_width="40dp"
            android:scaleType="fitXY"
            android:layout_height="40dp"/>

    </FrameLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
        <TextView
            android:id="@+id/tv_five"
            android:layout_gravity="center_vertical"
            android:padding="10dp"
            android:text="资深工作党"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <ImageButton
            android:id="@+id/five"
            android:padding="10dp"
            android:layout_gravity="center_vertical|right"
            android:background="#ffffff"
            android:src="@drawable/unselect_btn"
            android:layout_width="40dp"
            android:scaleType="fitXY"
            android:layout_height="40dp"/>

    </FrameLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
      <ImageButton
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:src="@mipmap/generation_finish"
          android:background="@null"
          android:layout_marginLeft="10dp"
          android:id="@+id/btn_enter_app"/>

    </FrameLayout>



</LinearLayout>